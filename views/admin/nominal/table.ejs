<table id="example" class="table table-striped" style="width:100%">
    <thead>
        <tr>
            <th style="text-align: left;">No.</th>
            <th>Name</th>
            <th>Qty</th>
            <th>Price</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% for (let i = 0; i < nominal.length; i++) { %>
            <tr>
                <td style="text-align: left;"><%= i + 1 %></td>
                <td><%= nominal[i].coinName %></td>
                <td><%= nominal[i].coinQuantity %></td>
                <td><%= formatToIDR(nominal[i].price) %></td>
                <td>
                    <form action="/nominal/delete/<%= nominal[i]._id %>?_method=DELETE" method="POST">
                        <a href="/nominal/edit/<%= nominal[i]._id %>" class="btn btn-sm btn-warning text-white">
                            <i class="bi bi-pencil m-1"></i>Edit</a>
                        <button type="submit" class="btn btn-sm btn-danger">
                            <i class="bi bi-trash m-1"></i>Delete</button>
                    </form>
                </td>
            </tr>
        <% } %>
    </tbody>
</table>